package poo;

public class Coche {

	//Atributos y con private se hace uso del encapsulamiento
	//solo se pueden modificar desde esta clase
	private int ruedas;
	private int largo;
	private int ancho;
	private int motor;
	private int peso;
	 //Constructor
	public Coche() {
		ruedas=4;
		largo=2000;
		ancho=300;
		motor=1600;
		peso=500;
	}
	
	//metodo getter
	
	public String getter_largo() {
		
		return "el largo del coche es" + largo;
	}
	
}
