
public class Arrays_Matrices {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//declaracion de array
		
		int [] matriz_1=new int[5];
		//int  matriz_1[]=new int[5];
		matriz_1[0]=1;
		matriz_1[1]=1;
		matriz_1[2]=1;
		matriz_1[3]=1;
		matriz_1[4]=1;
		// o asi
		//int [] matriz_1 = {1,1,1,1,1};
		for(int i=0;i<matriz_1.length;i++) {
			System.out.println("valor indice"+ i +" = "+matriz_1[i]);
			
		}
		
	}

}
